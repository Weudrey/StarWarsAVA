<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.labdesenvolvimento.atividadeava.MenuActivity">

    <com.facebook.login.widget.ProfilePictureView
        android:id="@+id/profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></com.facebook.login.widget.ProfilePictureView>

    <TextView
        android:id="@+id/textNome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/profile"
        android:layout_alignParentStart="true"/>

    <TextView
        android:id="@+id/textJSON"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="JsonAqui"
        android:layout_below="@+id/textNome"
        android:layout_toEndOf="@+id/textNome"
        android:layout_marginTop="45dp" />


</RelativeLayout>
